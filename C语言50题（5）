//打印出1~100以内的素数，每五个数字一行。

#include<stdio.h>
int main()
{
    int i,t,n=0;
    for(i=1;i<=100;a++)
    {
        for(t=2;t<=i;t++)
        {
            if((i%t)==0) break;
        }
        if(t==i)
        {
            printf("%3d",i);
            n++;
            if(n%5==0)
                printf("\n");
        }
    }
    return 0;
}
